{{ template "header" .}}
			<h1>Plugins</h1>
			<p>This section is for interacting with supported plugins. Not all plugins are supported, so don't be surprised if you don't see some plugins here.</p>
			<p><select id="pluginSelect" onchange="pluginSelect()">
				<option value="none">Plugin</option>
				{{ range .Plugins }}
					<option value="{{ . }}">{{ . }}</option>
				{{ end }}
			</select></p>
{{ template "footer"}}